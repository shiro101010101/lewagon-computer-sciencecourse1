    "import sqlite3\n",
    "\n",
    "conn = sqlite3.connect('data/movies.sqlite')\n",
    "db = conn.cursor()\n",
    "\n",
    "query = \"\"\"SELECT * FROM directors ,movies\n",
    "    WHERE  movies.start_year > death_year \n",
    "    LIMIT 10;\n",
    "    \"\"\"\n",
    "\n",
    "db.execute(query)\n",
    "rows=db.fetchall()\n",
    "print(rows[0][5])\n",
